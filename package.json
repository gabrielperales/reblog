    "watch-reason": "bsb -make-world -w"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "bs-platform": "^5.0.3",
    "bsb-js": "^1.1.7",
    "concurrently": "^4.1.0",
    "cssnano": "^4.1.10",
    "intersection-observer": "^0.6.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-imagemin": "^2.1.3",
    "parcel-plugin-md-fm": "^0.3.0",
    "react-snap": "^1.23.0"
  },
  "dependencies": {
    "@glennsl/bs-json": "^3.0.0",
    "@moox/bs-react-helmet": "^2.0.0",
    "bs-css": "^8.0.3",
    "bs-dynamic-import": "^0.3.0",
    "bs-webapi": "^0.14.4",
    "markdown-to-jsx": "^6.9.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.1",
    "react-highlight": "^0.12.0",
    "react-hook-intersection-observer": "^0.1.0",
    "reason-loadable": "^0.4.0",
    "reason-react": "^0.7.0",
    "svgo": "^1.2.2"
  },
  "reactSnap": {
    "source": "dist",
    "inlineCss": true,
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    }
  }
