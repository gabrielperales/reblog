{
  "name": "reblog",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postbuild": "react-snap",
    "start": "concurrently -c --kill-others 'npm:watch-html' 'npm:watch-reason'",
